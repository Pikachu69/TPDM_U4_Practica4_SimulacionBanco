<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/cajera"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="70dp"
        android:layout_height="70dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView3"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        android:background="@drawable/cajero"/>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        android:background="@drawable/cajera"/>

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        android:id="@+id/cliente1"/>

    <ImageView
        android:id="@+id/cliente2"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        android:id="@+id/cliente3"/>

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="@+id/cliente2"
        app:layout_constraintTop_toBottomOf="@+id/cliente2"
        android:id="@+id/c1c2"/>
    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toStartOf="@+id/c1c2"
        app:layout_constraintTop_toBottomOf="@+id/c1c2"
        android:id="@+id/c2c2"/>
    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toStartOf="@+id/c2c2"
        app:layout_constraintTop_toBottomOf="@+id/c2c2"
        android:id="@+id/c3c2"/>
    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toStartOf="@+id/c3c2"
        app:layout_constraintTop_toBottomOf="@+id/c3c2"
        android:id="@+id/c4c2"/>
    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toStartOf="@+id/c4c2"
        app:layout_constraintTop_toBottomOf="@+id/c4c2"
        android:id="@+id/c5c2"/>

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="@+id/cliente1"
        app:layout_constraintTop_toBottomOf="@+id/cliente1"
        android:id="@+id/c1c1"/>
    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toStartOf="@+id/c1c1"
        app:layout_constraintTop_toBottomOf="@+id/c1c1"
        android:id="@+id/c2c1"/>
    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toStartOf="@+id/c2c1"
        app:layout_constraintTop_toBottomOf="@+id/c2c1"
        android:id="@+id/c3c1"/>
    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toStartOf="@+id/c3c1"
        app:layout_constraintTop_toBottomOf="@+id/c3c1"
        android:id="@+id/c4c1"/>
    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toStartOf="@+id/c4c1"
        app:layout_constraintTop_toBottomOf="@+id/c4c1"
        android:id="@+id/c5c1"/>

    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="@+id/cliente3"
        app:layout_constraintTop_toBottomOf="@+id/cliente3"
        android:id="@+id/c1c3"/>
    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toStartOf="@+id/c1c3"
        app:layout_constraintTop_toBottomOf="@+id/c1c3"
        android:id="@+id/c2c3"/>
    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toStartOf="@+id/c2c3"
        app:layout_constraintTop_toBottomOf="@+id/c2c3"
        android:id="@+id/c3c3"/>
    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toStartOf="@+id/c3c3"
        app:layout_constraintTop_toBottomOf="@+id/c3c3"
        android:id="@+id/c4c3"/>
    <ImageView
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toStartOf="@+id/c4c3"
        app:layout_constraintTop_toBottomOf="@+id/c4c3"
        android:id="@+id/c5c3"/>

</android.support.constraint.ConstraintLayout>